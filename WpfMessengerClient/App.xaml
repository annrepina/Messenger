<Application x:Class="WpfChatClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfChatClient">
    <Application.Resources>
        
        
        <ResourceDictionary>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Styles/RoundedButton.xaml"/>
                <ResourceDictionary Source="./Styles/InputNameTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/InputPhoneNumberTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/InputPasswordTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/InputSecondPasswordTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/ExperimentalStyle.xaml"/>
                <ResourceDictionary Source="./Styles/InputSearchingNameTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/InputSearchingPhoneNumberTextBox.xaml"/>
                <ResourceDictionary Source="./Styles/InputGreetingMessageTextBox.xaml"/>
                <!--<ResourceDictionary Source="./Styles/ErrorStyle.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
